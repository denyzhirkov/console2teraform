<% if (albTargetGroups && albTargetGroups.length) { albTargetGroups.forEach(function(tg) { %>
<% if (tg.uniqueVarNames.tg_arn) { %>
variable "<%= tg.uniqueVarNames.tg_arn %>" {
  description = "Target Group ARN for ALB Target Group <%= tg.resourceName %>"
  type        = string
  default     = "<%= tg.targetGroupArn || '' %>"
}
<% } %>
<% }) } %> 