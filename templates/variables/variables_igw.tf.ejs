<% if (igws && igws.length) { igws.forEach(function(igw) { %>
variable "<%= igw.resourceName %>_vpc_id" {
  description = "VPC ID for IGW <%= igw.resourceName %>"
  type        = string
  default     = "<%= igw.vpcId || '' %>"
}
variable "<%= igw.resourceName %>_tags" {
  description = "Tags for IGW <%= igw.resourceName %>"
  type        = map(string)
  default     = {
<% Object.entries(igw.tags || {}).forEach(([k, v]) => { %>    <%= k %> = "<%= v %>"
<% }) %>  }
}
<% }) } %> 