<% function normalize(name) { return (name || '').replace(/[^a-zA-Z0-9_-]/g, '_').replace(/^[^a-zA-Z_]+/, '_'); } %>
<% if (ecsTaskDefs && ecsTaskDefs.length) { ecsTaskDefs.forEach(function(def) { %>
<% if (def.uniqueVarNames.taskdef_arn) { %>
variable "<%= def.uniqueVarNames.taskdef_arn %>" {
  description = "Task Definition ARN for ECS TaskDef <%= def.resourceName %>"
  type        = string
  default     = "<%= def.taskDefinitionArn || '' %>"
}
<% } %>
<% if (def.uniqueVarNames.family) { %>
variable "<%= def.uniqueVarNames.family %>" {
  description = "Family for ECS TaskDef <%= def.resourceName %>"
  type        = string
  default     = "<%= def.family || '' %>"
}
<% } %>
variable "<%= normalize(def.resourceName) %>_tags" {
  description = "Tags for ECS Task Definition <%= def.resourceName %>"
  type        = map(string)
  default     = {
<% Object.entries(def.tags || {}).forEach(([k, v]) => { %>    "<%= k.replace(/"/g, '\"') %>" = "<%= v %>"
<% }) %>  }
}
<% if (def.cpu) { %>variable "<%= normalize(def.resourceName) %>_cpu" {
  description = "CPU for ECS Task Definition <%= def.resourceName %>"
  type        = string
  default     = "<%= def.cpu %>"
}
<% } %><% if (def.memory) { %>variable "<%= normalize(def.resourceName) %>_memory" {
  description = "Memory for ECS Task Definition <%= def.resourceName %>"
  type        = string
  default     = "<%= def.memory %>"
}
<% } %><% if (def.networkMode) { %>variable "<%= normalize(def.resourceName) %>_network_mode" {
  description = "Network mode for ECS Task Definition <%= def.resourceName %>"
  type        = string
  default     = "<%= def.networkMode %>"
}
<% } %>
<% }) } %> 