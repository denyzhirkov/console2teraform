<% if (vpcs && vpcs.length) { %>
<% vpcs.forEach(function(vpc) { %>
output "<%= vpc.resourceName %>_vpc_id" {
  description = "ID of VPC <%= vpc.resourceName %>"
  value       = aws_vpc.<%= vpc.resourceName %>.id
}
<% }) %>
<% } %>

