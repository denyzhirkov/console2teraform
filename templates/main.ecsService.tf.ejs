<% if (ecsServices && ecsServices.length) { %>
<% ecsServices.forEach(function(service) { %>
resource "aws_ecs_service" "<%= service.resourceName %>" {
  name            = "<%= service.resourceName %>"
  cluster         = "<%= service.clusterArn %>"
  task_definition = "<%= service.taskDefinition %>"
  desired_count   = <%= service.desiredCount %>
<% if (service.launchType) { %>  launch_type = "<%= service.launchType %>"
<% } %><% if (service.tags) { %>  tags = {
<% Object.entries(service.tags).forEach(([k, v]) => { %>    <%= k %> = "<%= v %>"
<% }) %>  }
<% } %>}
<% }) %>
<% } %> 